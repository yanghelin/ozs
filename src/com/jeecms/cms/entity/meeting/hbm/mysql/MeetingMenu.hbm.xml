<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.cms.entity.meeting">
	<class name="MeetingMenu" table="ozs_meeting_menu" dynamic-update="true">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
    	<id name="id" type="java.lang.Integer" column="id"><generator class="identity"/></id>
	    <property column="name" name="name" type="string" />
	    <property column="url" name="url" type="string" />
	    <property column="is_delete" name="isDelete" type="java.lang.Byte" />
  </class>
</hibernate-mapping>